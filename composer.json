{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "ahmed-aliraqi/laravel-media-uploader": "^7.0",
        "algolia/algoliasearch-client-php": "^4.26",
        "anhskohbo/no-captcha": "^3.7",
        "astrotomic/laravel-translatable": "^11.10",
        "barryvdh/laravel-dompdf": "^2.0",
        "binarytorch/larecipe": "^2.5",
        "calebporzio/parental": "^1.1",
        "darkaonline/l5-swagger": "^8.0",
        "diglactic/laravel-breadcrumbs": "^v8.1.1",
        "doctrine/dbal": "^3.0",
        "fakerphp/faker": "^1.23",
        "guilhermegonzaga/presenter": "^1.0",
        "kreait/firebase-tokens": "^1.16",
        "lab404/laravel-impersonate": "^1.7",
        "laracasts/flash": "^3.2",
        "laraeast/laravel-settings": "^1.2",
        "laravel/framework": "^10.0",
        "laravel/sanctum": "^3.2",
        "laravel/scout": "^10.17",
        "laravel/tinker": "^2.7",
        "laravel/ui": "^4.2.2",
        "maatwebsite/excel": "^3.1",
        "milon/barcode": "^10.0.1",
        "propaganistas/laravel-phone": "^5.0.3",
        "pusher/pusher-php-server": "^7.0",
        "sendgrid/sendgrid": "^8.1",
        "spatie/laravel-activitylog": "^4.5",
        "spatie/laravel-backup": "^8.1",
        "spatie/laravel-medialibrary": "^10.15",
        "spatie/laravel-permission": "^5.5",
        "spatie/laravel-responsecache": "^7.6",
        "symfony/http-client": "^7.2",
        "symfony/mailgun-mailer": "^7.1",
        "symfony/sendgrid-mailer": "^7.1",
        "zircote/swagger-php": "^3.3"
    },
    "require-dev": {
        "ahmed-aliraqi/crud-generator": "^4.0",
        "barryvdh/laravel-debugbar": "^3.6",
        "barryvdh/laravel-ide-helper": "^2.12",
        "friendsofphp/php-cs-fixer": "^3.0",
        "laraeast/laravel-vue-i18n-generator": "^3.0",
        "laravel/sail": "^1.15",
        "mockery/mockery": "^1.5",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "php-cs:issues": "vendor/bin/php-cs-fixer fix --diff --dry-run",
        "php-cs:fix": "vendor/bin/php-cs-fixer fix",
        "app:clear": "php artisan clear-compiled && php artisan cache:clear && php artisan config:clear && php artisan debugbar:clear && php artisan route:clear && php artisan view:clear",
        "auto-complete:generate": [
            "@php artisan ide-helper:meta --ansi --quiet",
            "@php artisan ide-helper:generate --ansi --quiet",
            "@php artisan ide-helper:models --nowrite --quiet"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate --ansi --quiet",
            "@php artisan ide-helper:meta --ansi --quiet"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
